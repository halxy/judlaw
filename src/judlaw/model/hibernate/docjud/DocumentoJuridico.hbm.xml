<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	 
	<hibernate-mapping>
	
  		<class name="judlaw.model.docjud.DocumentoJuridico" table="documentojuridico">
  		
			<id  
			 	name="id" 
			 	type="java.lang.Integer"
			 	column="docjud_id">
			 	<generator class="increment"/>		 					
			</id>
			
			<one-to-one name="cabecalho" class="judlaw.model.docjud.Cabecalho" cascade="delete"/>
			
  		</class>
  </hibernate-mapping>