<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	 
	<hibernate-mapping>
	
  		<class name="judlaw.model.lei.ElementoNorma" table="elementonorma">
  		
			<id  
			 	name="id" 
			 	type="java.lang.Integer"
			 	column="elemento_id">
			 	<generator class="increment"/>		 					
			</id>
			
			<property
				name="identificadorUnico"
				type="java.lang.String"
			/>		
			
			<property
				name="tipoElemento"
				type="java.lang.String"
			/>	
			
			<property
				name="texto"
				type="java.lang.String"
			/>		
			
			<property
				name="data"
				type="java.lang.String"
			/>	
			
			<property
				name="vigencia"
				type="java.lang.String"
			/>	
			
			<property name="referenciasFeitas" type="java.util.ArrayList" />
			
			<property name="referenciasRecebidas" type="java.util.ArrayList" />
			
			<property name="elementosNorma" type="java.util.ArrayList" />
			
  		</class>
  </hibernate-mapping>